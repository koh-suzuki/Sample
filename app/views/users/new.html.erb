<div class="main users-new">
  <div class="container">
    <div class="form-heading">新規ユーザー登録</div>
    <div class="form users-form">
      <div class="form-body">
      <%= form_with url: users_path do |f| %>
        <p>ユーザー名</p>
        <%= f.text_field :name %>
        <p>メールアドレス</p>
        <%= f.email_field :email %>
        <%= f.submit value="新規登録" %>
      <% end %>
      </div>
    </div>
  </div>
</div>